Cell[TextData[{
 "The following function generates the operator from equation (16) from \
https://arxiv.org/abs/2301.10206 (",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SuperscriptBox[
        StyleBox["c", "TI"], 
        StyleBox["l", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "{c^l}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "9e02bf2b-d57d-4bc6-ad4c-7a34e3892fc7"],
 " is neglected and  ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SuperscriptBox[
        StyleBox["X", "TI"], 
        StyleBox["a", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "X^a", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "179dfe6a-b4b3-4e10-9567-a8d9bb3b9ff0"],
 " is replaced by ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SqrtBox[
          FractionBox[
           RowBox[{
             StyleBox["N", "TI"], 
             StyleBox["i", "TI"], 
             StyleBox["m", "TI"], "+", "1"}], 
           RowBox[{
             StyleBox["N", "TI"], 
             StyleBox["i", "TI"], 
             StyleBox["m", "TI"], "-", "1"}]]], 
         SuperscriptBox[
          StyleBox["X", "TI"], 
          StyleBox["a", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\sqrt{\\frac{Nim+1}{Nim-1}}X^a", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "9123e7d3-fc38-449d-b80e-7a8e6110561e"],
 ")"
}], "Text",
 CellChangeTimes->{{3.892723110897789*^9, 3.8927232074152155`*^9}, 
