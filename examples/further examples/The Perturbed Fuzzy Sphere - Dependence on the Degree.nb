Cell[BoxData[
 RowBox[{
  RowBox[{"Y", "[", 
   RowBox[{"l_", ",", "m_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"spherharm", "=", 
     RowBox[{"FullSimplify", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Sqrt", "[", 
           RowBox[{
            RowBox[{"xInternal", "^", "2"}], "+", 
            RowBox[{"yInternal", "^", "2"}], "+", 
            RowBox[{"zInternal", "^", "2"}]}], "]"}], ")"}], "^", "l"}], "*", 
        RowBox[{"Sqrt", "[", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"2", "l"}], "+", "1"}], ")"}], 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"l", "-", "m"}], ")"}], "!"}]}], ")"}], "/", 
          RowBox[{"(", 
           RowBox[{"4", "Pi", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"l", "+", "m"}], ")"}], "!"}]}], ")"}]}], "]"}], "*", 
        RowBox[{"LegendreP", "[", 
         RowBox[{"l", ",", "m", ",", 
          RowBox[{"zInternal", "/", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"xInternal", "^", "2"}], "+", 
             RowBox[{"yInternal", "^", "2"}], "+", 
             RowBox[{"zInternal", "^", "2"}]}], "]"}]}]}], "]"}], "*", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"xInternal", "+", 
               RowBox[{"I", "*", "yInternal"}]}], ")"}], "/", 
             RowBox[{"Sqrt", "[", 
              RowBox[{
               RowBox[{"xInternal", "^", "2"}], "+", 
               RowBox[{"yInternal", "^", "2"}], "+", 
               RowBox[{"zInternal", "^", "2"}]}], "]"}]}], ")"}], "/", 
           RowBox[{"Sqrt", "[", 
            RowBox[{"1", "-", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"zInternal", "/", 
                RowBox[{"Sqrt", "[", 
                 RowBox[{
                  RowBox[{"xInternal", "^", "2"}], "+", 
                  RowBox[{"yInternal", "^", "2"}], "+", 
                  RowBox[{"zInternal", "^", "2"}]}], "]"}]}], ")"}], "^", 
              "2"}]}], "]"}]}], ")"}], "^", "m"}]}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Element", "[", 
          RowBox[{"xInternal", ",", "Reals"}], "]"}], ",", 
         RowBox[{"Element", "[", 
          RowBox[{"yInternal", ",", "Reals"}], "]"}], ",", 
         RowBox[{"Element", "[", 
          RowBox[{"zInternal", ",", "Reals"}], "]"}]}], "}"}]}], "]"}]}], ";", 
    RowBox[{"(*", 
     RowBox[{
     "This", " ", "is", " ", "actually", " ", "done", " ", "by", " ", 
      "collecting", " ", "the", " ", "coefficients", " ", "of", " ", 
      "equation", " ", 
      RowBox[{"(", "15", ")"}], " ", "generated", " ", "via", " ", "the", " ",
       "Legendre", " ", "polynomials"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{"YCoeff", "=", 
     RowBox[{"SparseArray", "[", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", 
        RowBox[{"ConstantArray", "[", 
         RowBox[{"3", ",", "l"}], "]"}]}], "]"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"indices", "=", 
     RowBox[{"Tuples", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", "2", ",", "3"}], "}"}], ",", "l"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "1"}], ",", 
      RowBox[{"i", "<=", 
       RowBox[{"Length", "[", "indices", "]"}]}], ",", 
      RowBox[{"i", "++"}], ",", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"index", "=", 
          RowBox[{"indices", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], ";", 
         RowBox[{"xCount", "=", 
          RowBox[{"Count", "[", 
           RowBox[{"index", ",", "1"}], "]"}]}], ";", 
         RowBox[{"yCount", "=", 
          RowBox[{"Count", "[", 
           RowBox[{"index", ",", "2"}], "]"}]}], ";", 
         RowBox[{"zCount", "=", 
          RowBox[{"Count", "[", 
           RowBox[{"index", ",", "3"}], "]"}]}], ";", 
         RowBox[{"coeff", "=", 
          RowBox[{"Coefficient", "[", 
           RowBox[{"spherharm", ",", 
            RowBox[{
             RowBox[{"xInternal", "^", "xCount"}], "*", 
             RowBox[{"yInternal", "^", "yCount"}], "*", 
             RowBox[{"zInternal", "^", "zCount"}]}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"multiplicity", "=", 
          RowBox[{
           RowBox[{"l", "!"}], "/", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"xCount", "!"}], 
             RowBox[{"yCount", "!"}], 
             RowBox[{"zCount", "!"}]}], ")"}]}]}]}], ")"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"YCoeff", "=", 
        RowBox[{"ReplacePart", "[", 
         RowBox[{"YCoeff", ",", 
          RowBox[{"index", "->", 
           RowBox[{"coeff", "/", "multiplicity"}]}]}], "]"}]}]}]}], "]"}], 
    ";", "\[IndentingNewLine]", "YCoeff"}], "\[IndentingNewLine]", 
   ")"}]}]], "Input",
