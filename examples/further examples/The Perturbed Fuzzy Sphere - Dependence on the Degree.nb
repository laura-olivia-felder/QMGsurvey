Cell[BoxData[
 RowBox[{
  RowBox[{"qmgXsu2SphericalHarmonics", "[", 
   RowBox[{"Nim_", ",", "l_", ",", "m_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"l", "==", "0"}], ",", 
      RowBox[{"Ymat", "=", 
       RowBox[{"IdentityMatrix", "[", "Nim", "]"}]}], ",", 
      RowBox[{"(", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"X", "=", 
         RowBox[{
          RowBox[{"Sqrt", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"Nim", "+", "1"}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{"Nim", "-", "1"}], ")"}]}], "]"}], "*", 
          RowBox[{"qmgXsu2", "[", "Nim", "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Ymat", "=", 
         RowBox[{"ConstantArray", "[", 
          RowBox[{"0", ",", 
           RowBox[{"{", 
            RowBox[{"Nim", ",", "Nim"}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"YCoeff", "=", 
         RowBox[{"Y", "[", 
          RowBox[{"l", ",", "m"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"indices", "=", 
         RowBox[{"Tuples", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"1", ",", "2", ",", "3"}], "}"}], ",", "l"}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"i", "=", "1"}], ",", 
          RowBox[{"i", "<=", 
           RowBox[{"Length", "[", "indices", "]"}]}], ",", 
          RowBox[{"i", "++"}], ",", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"index", "=", 
             RowBox[{"indices", "[", 
              RowBox[{"[", "i", "]"}], "]"}]}], ";", 
            RowBox[{"coeff", "=", 
             RowBox[{"Extract", "[", 
              RowBox[{"YCoeff", ",", "index"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"coeff", "!=", "0"}], ",", 
              RowBox[{"Ymat", "=", 
               RowBox[{"Ymat", "+", 
                RowBox[{
                 RowBox[{"Extract", "[", 
                  RowBox[{"YCoeff", ",", "index"}], "]"}], "*", 
                 RowBox[{
                  RowBox[{"FoldList", "[", 
                   RowBox[{
                    RowBox[{
                    RowBox[{"#2", ".", "#1"}], "&"}], ",", 
                    RowBox[{"Table", "[", 
                    RowBox[{
                    RowBox[{"X", "[", 
                    RowBox[{"[", 
                    RowBox[{"index", "[", 
                    RowBox[{"[", "a", "]"}], "]"}], "]"}], "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"a", ",", "l", ",", "1", ",", 
                    RowBox[{"-", "1"}]}], "}"}]}], "]"}]}], "]"}], "[", 
                  RowBox[{"[", 
                   RowBox[{"-", "1"}], "]"}], "]"}]}]}]}]}], "]"}]}], 
           "\[IndentingNewLine]", ")"}]}], "]"}], ";", "\[IndentingNewLine]", 
        "Ymat"}], ")"}]}], "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"YmatNorm", "=", 
     RowBox[{"Sqrt", "[", 
      RowBox[{"Tr", "[", 
       RowBox[{
        RowBox[{"ConjugateTranspose", "[", "Ymat", "]"}], ".", "Ymat"}], 
       "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Ymat", "=", 
     RowBox[{"Ymat", "/", "YmatNorm"}]}]}], "\[IndentingNewLine]", 
   ")"}]}]], "Input",
