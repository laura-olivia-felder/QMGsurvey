66c8e9159235"],

Cell[BoxData[
 RowBox[{
  RowBox[{"StereographicPlot", "[", 
   RowBox[{"X_", ",", "xsets_"}], "]"}], ":=", 
  RowBox[{"(", "\n", 
   RowBox[{
    RowBox[{"Dim", "=", 
     RowBox[{"Length", "[", "X", "]"}]}], ";", 
    RowBox[{"(*", 
     RowBox[{"=", "D"}], "*)"}], "\n", 
    RowBox[{"Nim", "=", 
     RowBox[{"Length", "[", 
      RowBox[{"X", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", 
    RowBox[{"(*", 
     RowBox[{"=", "N"}], "*)"}], "\n", 
    RowBox[{"pointset", "=", 
     RowBox[{"{", "}"}]}], ";", 
    RowBox[{"(*", 
     RowBox[{
     "initialize", " ", "list", " ", "of", " ", "point", " ", "lists"}], 
     "*)"}], "\n", 
    RowBox[{"Colors", "=", 
     RowBox[{"{", "}"}]}], ";", 
    RowBox[{"(*", 
     RowBox[{
     "initialize", " ", "list", " ", "of", " ", "coloring", " ", "schemes"}], 
     "*)"}], "\n", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "1"}], ",", 
      RowBox[{"i", "<=", 
       RowBox[{"Length", "[", "xsets", "]"}]}], ",", 
      RowBox[{"i", "++"}], ",", 
      RowBox[{"(", 
       RowBox[{"(*", 
        RowBox[{"loop", " ", "over", " ", "all", " ", "sets"}], "*)"}], "\n", 
       RowBox[{
        RowBox[{"xs", "=", 
         RowBox[{"xsets", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "1"}], "]"}], "]"}]}], ";", 
        RowBox[{"(*", 
         RowBox[{"define", " ", "curent", " ", "xs"}], "*)"}], "\n", 
        RowBox[{"Color", "=", 
         RowBox[{"xsets", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "2"}], "]"}], "]"}]}], ";", 
        RowBox[{"(*", 
         RowBox[{"define", " ", "curent", " ", "Color"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"points", "=", 
         RowBox[{"StereographicProjection", "[", 
          RowBox[{"X", ",", "xs"}], "]"}]}], ";", 
        RowBox[{"(*", 
         RowBox[{"calculate", " ", "the", " ", "projection"}], "*)"}], "\n", 
        RowBox[{"pointset", "=", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"pointset", ",", "points"}], "]"}]}], ";", 
        RowBox[{"(*", 
         RowBox[{"append", " ", "Points", " ", "to", " ", "list"}], "*)"}], 
        "\n", 
        RowBox[{"Colors", "=", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"Colors", ",", "Color"}], "]"}]}], ";"}], 
       RowBox[{"(*", 
        RowBox[{"append", " ", "Color", " ", "to", " ", "list"}], "*)"}], 
       "\n", ")"}]}], "]"}], ";", "\n", 
    RowBox[{"plot", "=", 
     RowBox[{"ListPointPlot3D", "[", 
      RowBox[{"pointset", ",", 
       RowBox[{"PlotStyle", "->", "Colors"}], ",", 
       RowBox[{"PlotRange", "->", "All"}]}], "]"}]}], ";", "\n", 
    RowBox[{"{", 
     RowBox[{"plot", ",", "pointset", ",", "Colors"}], "}"}]}], "\n", 
   ")"}]}]], "Input",
 CellChangeTimes->{{3.8878579932550287`*^9, 3.8878579985419703`*^9}, {
