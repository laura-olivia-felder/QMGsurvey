Cell["\<\
We define a stereographic map, optimized to the geometry of a torus:\
\>", "Text",
 CellChangeTimes->{{3.8880670414986076`*^9, 
  3.88806706079885*^9}},ExpressionUUID->"1ee44856-33cf-466a-8a8c-\
1693922703b2"],

Cell[BoxData[
 RowBox[{
  RowBox[{"StereographicProjection", "[", 
   RowBox[{"X_", ",", "xs_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"x", "=", 
        RowBox[{"xs", "[", 
         RowBox[{"[", "a", "]"}], "]"}]}], ";", 
       RowBox[{"xBold", "=", 
        RowBox[{
         RowBox[{"qmgEmbedding", "[", 
          RowBox[{"X", ",", "x"}], "]"}], "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ";", 
       RowBox[{"xBoldProj", "=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Norm", "[", 
              RowBox[{"xBold", "[", 
               RowBox[{"[", 
                RowBox[{"1", ";;", "2"}], "]"}], "]"}], "]"}], "+", 
             RowBox[{"xBold", "[", 
              RowBox[{"[", "3", "]"}], "]"}]}], ")"}], "*", 
           RowBox[{
            RowBox[{"xBold", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "/", 
            RowBox[{"Norm", "[", 
             RowBox[{"xBold", "[", 
              RowBox[{"[", 
               RowBox[{"1", ";;", "2"}], "]"}], "]"}], "]"}]}]}], ",", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Norm", "[", 
              RowBox[{"xBold", "[", 
               RowBox[{"[", 
                RowBox[{"1", ";;", "2"}], "]"}], "]"}], "]"}], "+", 
             RowBox[{"xBold", "[", 
              RowBox[{"[", "3", "]"}], "]"}]}], ")"}], "*", 
           RowBox[{
            RowBox[{"xBold", "[", 
             RowBox[{"[", "2", "]"}], "]"}], "/", 
            RowBox[{"Norm", "[", 
             RowBox[{"xBold", "[", 
              RowBox[{"[", 
               RowBox[{"1", ";;", "2"}], "]"}], "]"}], "]"}]}]}], ",", 
          RowBox[{"xBold", "[", 
           RowBox[{"[", "4", "]"}], "]"}]}], "}"}]}], ";", "xBoldProj"}], 
      ")"}], ",", 
     RowBox[{"{", 
      RowBox[{"a", ",", "1", ",", 
       RowBox[{"Length", "[", "xs", "]"}]}], "}"}]}], "]"}], 
   "\[IndentingNewLine]", ")"}]}]], "Input",
 CellChangeTimes->{{3.878552281089178*^9, 3.8785524526388597`*^9}, 
   3.878552557358085*^9, {3.887857865648877*^9, 3.887857875095602*^9}, {
   3.8878579188474503`*^9, 3.887857978851866*^9}},
 CellLabel->"In[68]:=",ExpressionUUID->"4ef6daa7-17d9-48c6-ae7e-0c4d2245427a"],

